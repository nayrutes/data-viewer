<Application x:Class="WorldCompanyDataViewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WorldCompanyDataViewer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!--<ResourceDictionary Source="Themes/CustomStyle1.xaml"/>-->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/CustomStyle1.xaml" x:Name="custom"/>
                <ResourceDictionary Source="Themes/DefaultStyle.xaml" x:Name="default"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
